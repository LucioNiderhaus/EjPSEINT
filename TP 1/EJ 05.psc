Algoritmo sin_titulo
	Escribir "Ingresar cantidad de alfajores A:"
	Leer A
	Escribir "Ingresar cantidad de alfajores B:"
	Leer B
	Escribir "Ingresar cantidad de alfajores C:"
	Leer C
	TOTAL = A+B+C
	Q = (A*100)/TOTAL
	W = (B*100)/TOTAL
	E = (C*100)/TOTAL
	Escribir "A:",Q "%, " "B:",W "% y " "C:",E "%."
FinAlgoritmo
