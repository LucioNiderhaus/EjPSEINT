Algoritmo sin_titulo
	CONTADORP = 0
	CONTADORN = 0
	Leer N
	Mientras N!=0 Hacer
		Si N>0 Entonces
			CONTADORP = CONTADORP+1
		SiNo
			CONTADORN = CONTADORN+1
		FinSi
		Leer N
	FinMientras
	Escribir "LA CANTIDAD DE NUMERO POSITIVOS ES DE: ",CONTADORP," Y LA CANTIDAD DE NUMEROS NEGATIVOS ES DE: ",CONTADORN
FinAlgoritmo
